import io;
import math;
import file;

# Create a new file object based on users.txt, and create a list of all lines (1 user per line)
users = file("users.txt", "r").lines();

# Fetch random number between 0 and length of users list
idx = math.random(0, users.length());

# Print that user
io.print("Random user: ", users[idx], "\n");
